<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="True">
    <uie:Toolbar style="flex-direction: row-reverse;">
        <uie:ToolbarButton text="変更履歴" parse-escape-sequences="true" display-tooltip-when-elided="true" name="ChangeLogLink" style="-unity-text-align: middle-left; white-space: nowrap;" />
        <uie:ToolbarButton text="Github" parse-escape-sequences="true" display-tooltip-when-elided="true" name="GithubLink" style="justify-content: flex-start; -unity-text-align: middle-left;" />
    </uie:Toolbar>
    <ui:VisualElement style="flex-grow: 1; flex-direction: row;">
        <ui:VisualElement name="TabBar" style="flex-grow: 0; width: 35px; background-color: rgb(40, 40, 40); border-left-color: rgb(32, 32, 32); border-right-color: rgb(32, 32, 32); border-top-color: rgb(32, 32, 32); border-bottom-color: rgb(32, 32, 32); border-right-width: 2px;" />
