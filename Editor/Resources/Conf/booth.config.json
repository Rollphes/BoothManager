{
    "default": {
        "endpoint": {
            "protocol": "https",
            "domain": "booth.pm",
            "path": "/"
        }
    },
    "home": {
        "endpoints": {
            "home": {
                "path": "/"
            }
        }
    },
    "auth": {
        "endpoints": {
            "signIn": {
                "subDomain": "accounts",
                "domain": "pixiv.net",
                "path": "/login",
                "queryParams": {
                    "prompt": "select_account",
                    "return_to": "https%3A%2F%2Fbooth.pm%2Fusers%2Fauth%2Fpixiv",
                    "source": "booth",
                    "view_type": "page",
                    "_gl": "1*tye5an*_ga*NzMxMDk4NDI1LjE3MDcxODI0OTU.*_ga_RWT2QKJLDC*MTcwNzE4MjQ5NC4xLjEuMTcwNzE4MjUyNC4zMC4wLjA."
                }
            },
            "signUp": {
                "subDomain": "accounts",
                "domain": "pixiv.net",
                "path": "/signup",
                "queryParams": {
                    "prompt": "select_account",
                    "return_to": "https%3A%2F%2Fbooth.pm%2Fusers%2Fauth%2Fpixiv",
                    "source": "booth",
                    "view_type": "page",
                    "_gl": "1*tye5an*_ga*NzMxMDk4NDI1LjE3MDcxODI0OTU.*_ga_RWT2QKJLDC*MTcwNzE4MjQ5NC4xLjEuMTcwNzE4MjUyNC4zMC4wLjA."
                }
            }
        },
        "scripts": {
            "checkLoggedIn": "document.querySelector('.before-signin') === null"
        },
        "selectors": {
            "emailInput": "input[type=\"text\"]",
            "passwordInput": "input[type=\"password\"]",
            "errorMessage": ".sc-bn9ph6-5.fdOdXB",
            "nickName": ".global-nav-nickname"
        }
    },
    "order": {
        "endpoints": {
            "orderDetail": {
                "subDomain": "accounts",
                "path": "/orders/:orderId"
            }
        },
        "selectors": {
            "files": ".legacy-list-item",
            "fileName": ".u-flex-1.u-min-w-0.u-text-wrap.u-text-gray-500",
            "fileLink": "a"
        }
    },
    "api": {
        "endpoints": {
            "itemInfo": {
                "path": "/:lang/items/:itemId.json"
            },
            "likeList": {
                "path": "/wish_lists.json"
            },
            "wishList": {
                "subDomain": "accounts",
                "path": "/wish_list_name_items.json",
                "queryParams": {
                    "page": ":wishPageNumber"
                }
            }
        }
    },
    "library": {
        "endpoints": {
            "library": {
                "subDomain": "accounts",
                "path": "/library",
                "queryParams": {
                    "page": ":pageNumber"
                }
            },
            "gift": {
                "subDomain": "accounts",
                "path": "/library/gifts",
                "queryParams": {
                    "page": ":pageNumber"
                }
            }
        },
        "selectors": {
            "orders": ".mb-16.bg-white.p-16",
            "itemLink": ".flex.gap-8 a",
            "lastPageLink": "a.last-page"
        }
    }
}